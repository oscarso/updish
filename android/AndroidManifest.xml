<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="co.oscarsoft.updish"
    android:versionCode="2"
    android:versionName="1.1" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/up_512x512"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.NoTitleBar" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PlaceListActivity"
            android:label="@string/tit_activity_place_list" >
        </activity>
        <activity
            android:name=".ProductListActivity"
            android:label="@string/tit_activity_product_list" >
        </activity>
        <activity
            android:name=".PlaceAddActivity"
            android:label="@string/tit_activity_place_add" >
        </activity>
        <activity
            android:name=".ProductAddActivity1"
            android:label="@string/tit_activity_product_add" >
        </activity>
       <activity
            android:name=".ProductAddActivity2"
            android:label="@string/tit_activity_product_add" >
        </activity>
    </application>

</manifest>
